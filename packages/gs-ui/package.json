{
  "name": "gs-ui",
  "version": "0.1.0",
  "description": "基于Vue构建的UI组件库",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:components",
    "build:all": "vite build",
    "build:components": "esno ./scripts/build.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "lint": "eslint --fix --ext .ts,.tsx,.vue src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.vue\"",
    "changeset": "pnpm exec changeset",
    "changeset:version": "pnpm exec changeset version"
  },
  "keywords": [
    "Vite",
    "TypeScript"
  ],
  "author": "gzdl-strive",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@iconify-json/ic": "1.1.4",
    "@iconify-json/mdi": "^1.1.54",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "2.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.46.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.17.0",
    "happy-dom": "6.0.4",
    "prettier": "^3.0.1",
    "unocss": "0.45.6",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "0.21.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "vue": "3.2.37"
  },
  "files": [
    "dist"
  ]
}
